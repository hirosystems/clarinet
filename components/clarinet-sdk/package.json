{
  "name": "obscurity-sdk",
  "version": "0.3.5",
  "description": "clarity testing lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/hirosystems/clarinet"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./ts-gen": {
      "import": {
        "types": "./dist/esm/ts-gen/index.d.ts",
        "default": "./dist/esm/ts-gen/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/ts-gen/index.d.ts",
        "default": "./dist/cjs/ts-gen/index.js"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev-ts": "npm run build; webpack -c webpack.config.dev.js",
    "build": "npm run clean; webpack -c webpack.config.js",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "hirosystems",
  "license": "GPL-3.0",
  "readme": "./README.md",
  "dependencies": {
    "@stacks/transactions": "^6.5.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^5.0.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
