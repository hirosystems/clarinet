(define-data-var count int 0)
(define-private (print-count)
  (print (var-get count))
)
(define-read-only (get-count)
  (begin
    (print-count)
    (var-get count)
  )
)
(define-public (increment)
  (ok (var-set count (+ (var-get count) 1)))
)
(define-public (add (n int))
  (let ((new-count (+ (var-get count) n)))
    (print new-count)
    (ok new-count)
  )
)
